zabbix-username = admin
zabbix-password = secret
zabbix-url = http://zabbix.host.corp/

dashboard_limit = 20
dashboard_no_colors = 0
dashboard_priority_output = 0

searchWildcardsEnabled = 0

#ssh-key = ~/.ssh/production
#ssh-user = root
#ssh-options = -oForwardX11=yes

# cmd "U" arg
#ssh-custom-key = ~/.ssh/id_rsa
#ssh-custom-user = superuser

ansible_connection = ssh
ansible_ssh_user = root
ansible_ssh_private_key_file = ~/.ssh/id_rsa
ansible_python_interpreter = python

#debug = yes

mount-dir = ~/sshfs/$hostname
mount-options = allow_other

# Replace query space to .* and on regexp (-r)
replace-space = yes

alias test1 = test-1.host.corp
alias test2 = test-2.host.corp
